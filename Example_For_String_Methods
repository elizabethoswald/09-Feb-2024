public class Example_For_String_Methods {
	String myString="Hello Java World!";
	public void findingLength()
	{
		int length=myString.length();
		System.out.println("The length of the string "+myString+" is "+length);
	}
	public void findingWhatisAt()
	{
		char ch=myString.charAt(7);
		System.out.println("The character at 7th place is "+ch);
	}
	public void findingHashCode()
	{
		int hashcode=myString.hashCode();
        System.out.println("Hashcode of "+myString+" is "+hashcode);
	}
	public void findingUnicode()
	{
		int pointat=myString.codePointAt(7);
		System.out.println("The unicode of the 7th character is "+pointat);
	}
	public void findingUnicodeCount()
	{
		int count=myString.codePointCount(0,myString.length());
		System.out.println("The number of unicode values found in my String is "+count);
	}
	
	public static void main(String[] args) {
  		Example_For_String_Methods example=new Example_For_String_Methods();
  		example.findingLength();
  		example.findingWhatisAt();
  		example.findingHashCode();
  		example.findingUnicode();
  		example.findingUnicodeCount();
  }
}
